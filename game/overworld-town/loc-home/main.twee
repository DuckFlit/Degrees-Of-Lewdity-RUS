	<<switch _deco.name>>
		<<case "large bear plushie">>
			<<furnitureicon _deco.iconFile>>The _deco.name is sitting in a corner.
		<<default>>
			<<furnitureicon _deco.iconFile>>The _deco.name is hanging on a wall.
	<</switch>>
<</if>>

<<set _owlplush to Furniture.get('owlplushie')>>
<<if _owlplush>>
