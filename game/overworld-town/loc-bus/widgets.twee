	<<rng 13>>
	<<switch $rng>>
		<<case 1>><<set $bus to "nightingale">>
		<<case 2>><<set $bus to "domus">>
		<<case 3>><<set $bus to "elk">>
		<<case 4>><<set $bus to "high">>
		<<case 5>><<set $bus to "starfish">>
		<<case 6>><<set $bus to "barb">>
		<<case 7>><<set $bus to "connudatus">>
